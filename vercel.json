{
  "version": 2,
  "name": "tesla-model-pi",
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "server/index.ts" },
    { "src": "/(.*)", "dest": "server/index.ts" }
  ]
}
